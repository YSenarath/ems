{% extends 'AppBundle::base.html.twig' %}




{% block otherstylesheets %}

    <link rel="stylesheet" href="{{ asset('bundles/framework/css/sensorList.css') }}"/>

{% endblock %}

{% block body %}

    <h1 align="center">
        Sensor Types
    </h1>
    <div class="container">


        <div class="row" id = "typeTable">
            <table class="table table-striped">
                <tr>
                    <th>Type</th>
                    <th>Response Interval</th>
                </tr>

                {% for t in types  %}
                    <tr>
                        {% if t.typeName|trim is not empty %}
                            <td>{{t.typeName }}</td>

                            {% if t.getResInterval|trim is not empty %}
                                <td>{{ t.getResInterval }} | <a href="#" >change</a> </td>
                            {% else %}
                                <td >Not Set</td>
                            {% endif %}

                        {% endif %}
                    </tr>
                {% endfor %}
            </table>
        </div>

    </div>



{% endblock %}